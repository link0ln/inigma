# Environment variables for development
# Copy this file to .env and fill in your values

# Domain for the application
DOMAIN=inigma.idone.su

# Days after which messages are cleaned up
CLEANUP_DAYS=50

# Cloudflare API Token (for wrangler)
# CLOUDFLARE_API_TOKEN=your_api_token_here

# R2 bucket name
R2_BUCKET_NAME=inigma-storage

# KV namespace ID (optional, for analytics)
# KV_NAMESPACE_ID=your_kv_namespace_id_here
